FRAMEWORK_SEARCH_PATHS = $(inherited) "$(PODS_ROOT)/Bugtags/"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AFViewShaker" "${PODS_ROOT}/Headers/Public/Bugtags" "${PODS_ROOT}/Headers/Public/OpenShare" "${PODS_ROOT}/Headers/Public/Shimmer" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AFViewShaker" -isystem "${PODS_ROOT}/Headers/Public/Bugtags" -isystem "${PODS_ROOT}/Headers/Public/OpenShare" -isystem "${PODS_ROOT}/Headers/Public/Shimmer" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = $(inherited) -ObjC -l"AFViewShaker" -l"OpenShare" -l"Shimmer" -l"c++" -l"pop" -framework "AVFoundation" -framework "Bugtags" -framework "CFNetwork" -framework "CoreLocation" -framework "ImageIO" -framework "Security" -framework "SystemConfiguration" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods